<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.embed.swing.SwingNode?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="jaicore.search.gui.FXController"
            prefHeight="400.0" prefWidth="600.0">

    <top>
        <BorderPane>
            <top>
                <ToolBar>
                    <Button text="play" onAction="#play"/>
                    <Button text="step" onAction="#step"/>
                    <Button text="back" onAction="#back"/>
                    <Button text="reset" onAction="#reset"/>
                    <Button text="stop" onAction="#stop"/>
                    <Button text="save" onAction="#save"/>
                    <Button text="load" onAction="#load"/>
                </ToolBar>
            </top>
            <bottom>
                <Slider fx:id="slider">
                    <min>0</min>
                    <max>200</max>
                    <value>150</value>
                    <showTickMarks>true</showTickMarks>
                    <showTickLabels>true</showTickLabels>
                    <majorTickUnit>10</majorTickUnit>
                    <snapToTicks>true</snapToTicks>
                </Slider>
            </bottom>
        </BorderPane>
    </top>

    <center>
        <SwingNode fx:id="swingNode"/>
    </center>


    <bottom>

    </bottom>
</BorderPane>
