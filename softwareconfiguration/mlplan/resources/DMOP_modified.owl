    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#abstract-region -->

    <owl:Class rdf:about="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#abstract-region">
        <rdfs:subClassOf rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#region"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#part"/>
                <owl:allValuesFrom rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#abstract-region"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#q-location-of"/>
