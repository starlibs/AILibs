    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#abstract-quality -->

    <owl:Class rdf:about="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#abstract-quality">
