    <!-- http://www.e-lico.eu/ontologies/dmo/DMOP/PD.owl#SubgroupDiscoveryAlgorithm -->

    <owl:Class rdf:about="http://www.e-lico.eu/ontologies/dmo/DMOP/PD.owl#SubgroupDiscoveryAlgorithm">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.e-lico.eu/ontologies/dmo/DMOP/DMOP.owl#PatternDiscoveryAlgorithm"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.e-lico.eu/ontologies/dmo/DMOP/DMOP.owl#addresses"/>
                        <owl:someValuesFrom rdf:resource="http://www.e-lico.eu/ontologies/dmo/DMOP/DMOP.owl#SubgroupDiscoveryTask"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.e-lico.eu/ontologies/dmo/DMOP/DMOP.owl#hasOptimizationStrategy"/>
                <owl:someValuesFrom rdf:resource="http://www.e-lico.eu/ontologies/dmo/DMOP/DMOP.owl#OptimizationStrategy"/>
            </owl:Restriction>
